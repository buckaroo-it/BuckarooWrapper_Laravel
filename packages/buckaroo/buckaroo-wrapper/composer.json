{
    "name": "buckaroo/buckaroowrapper",
    "description": "Laravel Wrapper package for Buckaroo Payments Gateway",
    "license": "MIT",
    "authors": [
        {
            "name": "Amar Beka",
            "email": "a.beka@buckaroo-kos.com"
        }
    ],
    "require": {
        "buckaroo/sdk": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Buckaroo\\BuckarooWrapper\\" : "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Buckaroo\\BuckarooWrapper\\BuckarooServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
